{
  "name": "brightnucleus/service-locator",
  "description": "Config-driven Service Locator, based on Pimple",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phpunit/phpunit": "~4",
    "php-composter/php-composter-phpcs-psr2": ">=0.2.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Alain Schlesser",
      "email": "alain.schlesser@gmail.com"
    }
  ],
  "require": {
    "brightnucleus/exceptions": ">=0.2",
    "brightnucleus/config": ">=0.4.7",
    "brightnucleus/injector": ">=0.3",
    "psr/log": "^1",
    "pimple/pimple": "^3",
    "container-interop/container-interop": "^1"
  },
  "autoload": {
    "psr-4": {
      "BrightNucleus\\ServiceLocator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BrightNucleus\\ServiceLocator\\Test\\": "tests/"
    }
  }
}
